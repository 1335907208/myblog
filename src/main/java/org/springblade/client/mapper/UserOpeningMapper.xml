<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.client.mapper.UserOpeningMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="userOpeningResultMap" type="org.springblade.client.entity.UserOpening">
        <id column="id" property="id"/>
        <result column="create_user" property="createUser"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="create_time" property="createTime"/>
        <result column="status" property="status"/>
        <result column="project_id" property="projectId"/>
        <result column="prev" property="prev"/>
        <result column="curent" property="curent"/>
        <result column="nexte" property="nexte"/>
        <result column="nextnexte" property="nextnexte"/>
    </resultMap>


    <select id="selectUserOpeningPage" resultMap="userOpeningResultMap">
        select * from house_user_opening where is_deleted = 0
    </select>

</mapper>
