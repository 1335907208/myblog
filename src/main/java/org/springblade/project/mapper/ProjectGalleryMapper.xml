<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.project.mapper.ProjectGalleryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="projectGalleryResultMap" type="org.springblade.project.entity.ProjectGallery">
        <id column="id" property="id"/>
        <result column="create_user" property="createUser"/>
        <result column="create_dept" property="createDept"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="gallery_type" property="galleryType"/>
        <result column="project_id" property="projectId"/>
        <result column="play_time" property="playTime"/>
        <result column="image_list" property="imageList"/>
        <result column="remark" property="remark"/>
    </resultMap>


    <select id="selectProjectGalleryPage" resultMap="projectGalleryResultMap">
        select * from house_project_gallery where is_deleted = 0
    </select>

</mapper>
