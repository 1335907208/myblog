<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springblade.modules.agency.mapper.AgencyProjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="agencyProjectResultMap" type="org.springblade.modules.agency.entity.AgencyProject">
        <id column="id" property="id"/>
        <result column="create_user" property="createUser"/>
        <result column="create_dept" property="createDept"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="status" property="status"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="agency_id" property="agencyId"/>
        <result column="project_id" property="projectId"/>
    </resultMap>


    <select id="selectAgencyProjectPage" resultMap="agencyProjectResultMap">
        select * from house_agency_project where is_deleted = 0
    </select>

</mapper>
